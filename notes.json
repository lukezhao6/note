      {
        "title": "转换时间格式",
        "content": "DATE_FORMAT(created_at,'%Y-%m-%d')"
      },
      {
        "title": "当前时间减少8天",
        "content": "DATE_SUB(NOW(), INTERVAL 8 DAY)"
